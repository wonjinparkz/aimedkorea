            "origin": "https://ai-med.co.kr"
        },
        {
            "origin": "*.aimedkorea.com"
        }
    ],
    "launch_handler": {
        "client_mode": ["navigate-existing", "auto"]
    },
    "edge_side_panel": {
        "preferred_width": 480
    },
