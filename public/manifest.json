        {
            "action": "/open-file",
            "accept": {
                "text/csv": [".csv"],
                "application/pdf": [".pdf"]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+aimed",
            "url": "/protocol?url=%s"
        }
    ],
    "note_taking": {
        "new_note_url": "/notes/new"
    },
